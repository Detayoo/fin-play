{
  "expo": {
    "name": "uzzy-v1",
    "slug": "uzzy-v1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "uzzy-v1",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/uzzy-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow UZZY to use Face ID."
      },
      "bundleIdentifier": "com.adedigba.uzzyv1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.adedigba.uzzyv1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow UZZY to use Face ID."
        }
      ],
      ["expo-router"],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Uzzy to access your contacts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ee9231cd-c8b1-4ba1-91d9-8267d6bc0cc6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ee9231cd-c8b1-4ba1-91d9-8267d6bc0cc6"
    }
  }
}
